{
  "name": "rest-server-core",
  "version": "1.0.0",
  "description": "the rest-server core of openi",
  "apidoc": {
    "title": "Custom apiDoc browser title",
    "url": ""
  },
  "private": true,
  "dependencies": {
    "apidoc": "^0.17.7",
    "bluebird": "^3.5.3",
    "egg-cors": "^2.1.2",
    "egg-opener": "^1.0.1",
    "egg-router-plus": "^1.3.0",
    "egg-scripts": "^2.5.0",
    "egg-security": "^2.4.1",
    "egg-sequelize": "^4.2.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "koa-compress": "^3.0.0",
    "koa-jwt": "3.5.1",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "marked": "^0.6.2",
    "mustache": "^3.0.1",
    "mysql2": "^1.6.4",
    "nanoid": "^2.0.1",
    "nodemailer": "^6.3.1",
    "request": "^2.88.0",
    "unirest": "^0.6.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "npm run doc && egg-scripts start --workers=3 --title=egg-rest-server-core  --framework=egg-opener",
    "stop": "egg-scripts stop --title=egg-rest-server-core",
    "dev": "npm run doc && egg-bin dev --framework=egg-opener",
    "debug": "egg-bin debug  --framework=egg-opener",
    "test": "echo 'go testing' || $(npm run lint -- --fix && npm run test-local)",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "doc": "apidoc -i app/routes -i lib/error -o app/apidoc/"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
